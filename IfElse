from unittest.test.test_result import m

x = 8
r = x % 2
if r == 0:
    print('EVEN')
    if x > 5:
        print('Graet')
    else:
        print('Not So Great')
else:
    print('ODD')
    print('BY')



print('Enter A Number:')
n = eval(input())
m = list(list(range(1 * i,(n + 1) * i,i)) for i in range(1,n + 1))
for i in m:
    print('\n')
    i = [str(j).rjust(len(str(m[-1][-1])) + 1) for j in i]
    print('   '.join(i))

